{
  "name": "@flaticon/flaticon-uicons",
  "description": "Interface icons from Flaticon",
  "version": "1.0.0",
  "main": "index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "ts-types": " tsc --emitDeclarationOnly --outDir lib",
    "clean:css": "rimraf css",
    "clean:svg": "rimraf svg",
    "prebuild": "run-p clean:css clean:svg",
    "build:css": "node ./esbuild.js",
    "build:svg": "svgo -f ./src/uicons/svg -o svg",
    "build": "run-p ts-types build:css build:svg",
    "postinstall": "node license"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/graphicresources/flaticon_uicons.git"
  },
  "keywords": [],
  "author": "gCuadros",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/graphicresources/flaticon_uicons/issues"
  },
  "homepage": "https://bitbucket.org/graphicresources/flaticon_uicons#readme",
  "devDependencies": {
    "esbuild": "^0.14.5",
    "esbuild-plugin-glob": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "svgo": "^2.8.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "esbuild-linux-64": "^0.14.5"
  },
  "private": false

}
